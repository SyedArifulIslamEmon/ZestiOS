EMBEDDED_CONTENT_CONTAINS_SWIFT = YES
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/AFNetworking.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/AZDropdownMenu.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Firebase.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/HexColors.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Moltin.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/SDWebImage.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/SwiftSpinner.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/UberRides.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "AFNetworking" -framework "AZDropdownMenu" -framework "Firebase" -framework "HexColors" -framework "Moltin" -framework "SDWebImage" -framework "SwiftSpinner" -framework "UberRides"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods
PODS_ROOT = ${SRCROOT}/Pods